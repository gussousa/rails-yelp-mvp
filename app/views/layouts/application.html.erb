<!DOCTYPE html>
<html>
  <head>
    <title>RailsYelpMvp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="navbar navbar-light bg-light mb-4 p-4">
      <div>
        <%= link_to "Restaurant Index", restaurants_path %>
      </div>
      <div>
        <%= link_to "Create Restaurant", new_restaurant_path %>
      </div>
    </div>
    <%= yield %>
  </body>
</html>
